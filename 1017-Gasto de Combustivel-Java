package beecrowd;

import java.util.Scanner;

public class Bee1017 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int km = 12;
		int tempo = 0, vm = 0;
		float litros;
		
		tempo = sc.nextInt();
		vm = sc.nextInt();
		
		float distancia = vm/tempo;
		
		litros = distancia*km;
		
		System.out.println(String.format("%.3f", litros));
		
		sc.close();
	}

}
